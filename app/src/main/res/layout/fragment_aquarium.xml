<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.aquarium.AquariumFragment">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.airbnb.lottie.LottieAnimationView
            android:layout_width="match_parent"
            android:layout_height="405dp"
            android:layout_above="@id/iv_underTheSea"
            android:layout_alignParentEnd="true"
            android:layout_marginBottom="-60dp"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/sea2" />

        <ImageView
            android:id="@+id/iv_underTheSea"
            android:layout_width="match_parent"
            android:layout_height="300dp"
            android:layout_alignParentBottom="true"
            android:background="#78d8df"
            />

        <com.airbnb.lottie.LottieAnimationView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            app:lottie_autoPlay="true"
            android:layout_marginBottom="-180dp"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/bubble"/>


        <ImageView
            android:id="@+id/iv_fish"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@drawable/fish2"
            android:layout_marginBottom="20dp"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"/>
        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/iv_cuteFish"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginBottom="20dp"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/cutefish"
            />
        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/iv_fishShark"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_marginBottom="20dp"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/fish_shark"
            />
        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/iv_fishSpinJelly"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginBottom="20dp"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/fish_spin_jelly"
            />
        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/iv_fishTurtle"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginBottom="20dp"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/fish_turtle"
            />
        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/iv_fishWhale"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginBottom="20dp"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/fish_whale"
            />
        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/iv_fishBalloon"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_marginBottom="20dp"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/fish_balloon"
            />
        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/iv_fishBlue"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginBottom="20dp"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/fish_blue"
            />



    </RelativeLayout>
</FrameLayout>